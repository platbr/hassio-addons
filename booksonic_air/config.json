{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "platform for accessing the audibooks you own wherever you are",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "PGID": "0",
    "PUID": "0"
  },
  "init": false,
  "map": [
    "share:rw",
    "media:rw"
  ],
  "name": "Booksonic-air",
  "options": {
    "PGID": 0,
    "PUID": 0,
    "cifspassword": "<password>",
    "cifsusername": "<username>",
    "networkdisks": "<//SERVER/SHARE>,<//SERVER2/SHARE2>"
  },
  "ports": {
    "4040/tcp": 4040
  },
  "ports_description": {
    "4040/tcp": "web interface"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PGID": "int",
    "PUID": "int",
    "TZ": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "localdisks": "str?",
    "networkdisks": "str?"
  },
  "slug": "booksonic-air_nas",
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/booksonic_air",
  "version": "v2201.1.0-ls58",
  "webui": "http://[HOST]:[PORT:4040]"
}
